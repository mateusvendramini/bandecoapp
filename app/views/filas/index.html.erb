<p id="notice"><%= notice %></p>

<h1>Filas</h1>

<table>
  <thead>
    <tr>
      <th>Estado fila</th>
      <th>Id restaurante</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @filas.each do |fila| %>
      <tr>
        <td><%= fila.estado_fila %></td>
        <td><%= fila.id_restaurante %></td>
        <td><%= link_to 'Show', fila %></td>
        <td><%= link_to 'Edit', edit_fila_path(fila) %></td>
        <td><%= link_to 'Destroy', fila, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fila', new_fila_path %>
